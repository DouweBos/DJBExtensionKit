{
  "name": "DJBExtensionKit",
  "version": "0.4.5",
  "summary": "Collection of personal extension I like to use.",
  "description": "Just a collection of extensions I like to use. This is just so I no longer have to copy everything over from project to project. Please do not actually use me.",
  "homepage": "https://github.com/DouweBos/DJBExtensionKit",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "DouweBos": "douwe@douwebos.nl"
  },
  "source": {
    "git": "https://github.com/DouweBos/DJBExtensionKit.git",
    "tag": "0.4.5"
  },
  "platforms": {
    "ios": "10.0",
    "tvos": "10.0"
  },
  "swift_versions": "5.1",
  "default_subspecs": "Core",
  "subspecs": [
    {
      "name": "Core",
      "source_files": "DJBExtensionKit/Classes/Core/**/*"
    },
    {
      "name": "RxSwift",
      "source_files": "DJBExtensionKit/Classes/RxSwift/**/*",
      "dependencies": {
        "RxSwift": [
          "~> 5"
        ],
        "RxSwiftExt": [
          "~> 5"
        ],
        "RxCocoa": [
          "~> 5"
        ]
      }
    },
    {
      "name": "Kingfisher",
      "source_files": "DJBExtensionKit/Classes/Kingfisher/**/*",
      "dependencies": {
        "Kingfisher": [

        ]
      }
    }
  ],
  "swift_version": "5.1"
}
